<?xml version="1.0" encoding="UTF-8"?>
    <iso_project
        image_name="build/game.iso"
        cue_sheet="build/game.cue"
        no_xa="0"
    >
        <track type="data">
            <identifiers
                system			= "PLAYSTATION"
                application		= "PLAYSTATION"
                volume			= "DISC"
                volume_set		= "GAME"
                publisher		= "ELIASDALER"
                data_preparer	= "MKPSXISO"
                copyright		= "NONE"
            />
            <! license file is only needed for running on JP consoles >
            <! license file="LICENSEJ.DAT"/>
            <directory_tree>
                <file name="SYSTEM.CNF"	type="data" source="system.cnf"/>
                <file name="GAME.EXE" type="data" source="build/game.ps-exe"/>
                <file name="BRICKS.TIM" type="data" source="assets/bricks.tim"/>
                <file name="LEVEL.BIN" type="data" source="assets/house_psx.bin"/>
                <file name="HUMAN.BIN" type="data" source="assets/human.bin"/>
                <file name="STEP1.VAG" type="data" source="assets/step1.vag"/>
                <file name="CATO.BIN" type="data" source="assets/cato.bin"/>
                <file name="CATO.TIM" type="data" source="assets/cato.tim"/>
                <dummy sectors="1024"/>
            </directory_tree>
        </track>
</iso_project>

