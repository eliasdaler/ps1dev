<?xml version="1.0" encoding="UTF-8"?>
    <iso_project
        image_name="build/game.iso"
        cue_sheet="build/game.cue"
        no_xa="0"
    >
        <track type="data">
            <identifiers
                system			= "PLAYSTATION"
                application		= "PLAYSTATION"
                volume			= "DISC"
                volume_set		= "GAME"
                publisher		= "ELIASDALER"
                data_preparer	= "MKPSXISO"
                copyright		= "NONE"
            />
            <! license file="a\license_data.dat"/>
            <directory_tree>
                <file name="SYSTEM.CNF"	type="data" source="system.cnf"/>
                <file name="GAME.EXE" type="data" source="build/game.ps-exe"/>
                <file name="ROLL.BIN" type="data" source="assets/roll.bin"/>
                <file name="ROLL.TIM" type="data" source="assets/roll.tim"/>
                <file name="BRICKS.TIM" type="data" source="assets/bricks.tim"/>
                <file name="LEVEL.BIN" type="data" source="assets/level.bin"/>
                <file name="FTEX.TIM" type="data" source="assets/ftex.tim"/>
                <file name="PS1.TIM" type="data" source="assets/ps1.tim"/>
                <dummy sectors="1024"/>
            </directory_tree>
        </track>
</iso_project>

